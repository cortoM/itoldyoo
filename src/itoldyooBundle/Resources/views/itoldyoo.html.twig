<!DOCTYPE html>
<html lang="en" ng-app="iToldYooApp">
    <head>
        <meta charset="UTF-8" />
        <title>
        {% block title %}
        {{ "global.application.name"|trans }}
        {% endblock %}
        </title>
        {% block stylesheets %}
            <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
            <link rel="stylesheet" href="../bundles/itoldyoo/bower_components/bootstrap/dist/css/bootstrap.css">
            <!--  <link rel="stylesheet" href="css/bootstrap.css">
            <link rel="stylesheet" href="css/bootstrap-theme.css"> -->
            <link rel="stylesheet" href="../bundles/itoldyoo/css/itoldyoo.css">
            <!--<link rel="stylesheet" href="bundles/itoldyoo/css/test.css"> -->
        {% endblock %}
        {% block javascripts %}
            <script src="../bundles/itoldyoo/bower_components/jquery/dist/jquery.js"></script>
            <script src="../bundles/itoldyoo/bower_components/angular/angular.js"></script>
            <script src="../bundles/itoldyoo/bower_components/angular-route/angular-route.js"></script>
            <script src="../bundles/itoldyoo/bower_components/angular-resource/angular-resource.js"></script>
            <!--<script type="text/javascript" src="bower_components/bootstrap/js/carousel.js"></script> -->
            <script src="../bundles/itoldyoo/js/lib/bootstrap.min.js"></script>
            <script src="../bundles/itoldyoo/js/app.js"></script>
            <script src="../bundles/itoldyoo/js/controllers.js"></script>
            <script src="../bundles/itoldyoo/js/filters.js"></script>
            <script src="../bundles/itoldyoo/js/services.js"></script>
            <script src="../bundles/itoldyoo/js/components.js"></script>
            <script src="../bundles/itoldyoo/js/datetimeservices.js"></script>
        {% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}
    </body>
</html>